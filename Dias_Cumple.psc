Algoritmo Dias_Cumple
	Escribir "Introduce el día de tu nacimiento"
	Leer dia_n
	Escribir "Introduce el mes de tu nacimiento"
	Leer mes_n
	Escribir "Introduce el año de tu nacimiento"
	Leer año_n
	Escribir "Introduce el día actual"
	Leer dia_a
	Escribir "Introduce el mes actual"
	Leer mes_a
	Escribir "Introduce el año actual"
	Leer año_a
	Si año_a>año Entonces
		Si año_a%4==0 Entonces
		
		FinSi
	SiNo
		Escribir "Fecha no coincide!"
	FinSi
FinAlgoritmo
